{
  "manifest_version": 2,
  "description": "This is stock monitor app",
  "version": "0.0.1",
  "homepage_url": "https://github.com/Jongkeun/stock-monitor",
  "short_name": "stock-monitor",
  "name": "stock-monitor",
  "icons": {
    "16": "icons/logo-small.png",
    "48": "icons/logo-small.png",
    "128": "icons/logo-small.png"
  },
  "browser_action": {
    "default_title": "stock"
  },
  "background":
  {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs",
    "activeTab",
    "webRequest",
    "contextMenus",
    "downloads",
    "webNavigation",
    "notifications",
    "storage",
    "debugger",
    "<all_urls>"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'"
}
